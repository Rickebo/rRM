<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net6.0-windows</TargetFramework>
        <Nullable>enable</Nullable>
        <UseWPF>true</UseWPF>
        <ApplicationIcon>v9g-icon-multisize.ico</ApplicationIcon>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.PowerShell.SDK" Version="6.2.7" />
      <PackageReference Include="Pcap.Net.x64" Version="1.0.4.1" />
      <PackageReference Include="System.Management.Automation" Version="6.2.7" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="v9g-icon-multisize.ico" />
      <Resource Include="v9g-icon-multisize.ico" />
    </ItemGroup>

</Project>
